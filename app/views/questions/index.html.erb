<p id="notice"><%= notice %></p>

<% @questions.each do |question| %>
	<h2><%= question.num %> | <%= link_to question.subject, question %> | <%= question.score %> | <%= question.exam.name %> | <%= link_to 'Edit', edit_question_path(question), 'data-no-turbolink' => true %> | <%= link_to 'Destroy', question, method: :delete, data: { confirm: 'Are you sure?' } %></h2>
<% end %>

